	//for loop to get the cell data
	for(int i=1;i<Rows;i++) {

			Cell Exccel1 = Excelsheet.getCell(0,i);
			//get contents from String array
//			Testdata[count][j]= Exccel1.getContents();
		}
		count++;
	}
	Excelfile.close();
	return Testdata;
	}
